  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "ansi_up": "^5.2.1",
    "gsap": "^3.12.2",
    "highlight.js": "^11.8.0",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-material-design-icons": "^5.2.0",
    "vue-router": "^4.2.4"
