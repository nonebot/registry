  "packageManager": "pnpm@10.6.2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:preview": "vue-tsc && vite build --mode preview",
